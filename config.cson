"*":
  "atom-beautify":
    general:
      _analyticsUserId: "668aabdf-da30-44a6-88ee-9a8c4053e5f7"
    html:
      end_with_newline: true
    php:
      standard: "PSR2"
  "atom-ctags":
    buildTimeout: 360000
    cmdArgs: "--exclude=node_modules --exclude=cache"
  core:
    disabledPackages: [
      "language-gfm"
      "wrap-guide"
      "linter-csslint"
      "linter-sass-lint"
      "linter-scss-lint"
      "metrics"
      "exception-reporting"
      "markdown-scroll-sync"
      "symbols-tree-view"
      "code-peek"
      "livereload"
      "atom-autocomplete-php"
      "activate-power-mode"
      "php-integrator-refactoring"
      "autoclose-html"
      "imdone-atom"
      "php-integrator-symbol-viewer"
      "symbols-view"
    ]
    excludeVcsIgnoredPaths: false
    followSymlinks: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "logs"
      "**/public/assets/*"
      "**/storage/framework/views/*"
      "**/public/build/*"
      ".tags"
      ".tags1"
      ".tags2"
      ".tags3"
      ".tags4"
      ".tags*"
    ]
    projectHome: "/Users/boris/Code"
    telemetryConsent: "limited"
  "custom-title":
    template: "<%= fileName %><% if (projectName) { %> - <%= projectName %><% if (gitHead) { %> [<%= gitHead %>]<% } %><% } %>"
  docblockr:
    return_description: false
  editor:
    fontFamily: "Operator Mono"
    fontSize: 12
    invisibles: {}
    lineHeight: 3
    scrollPastEnd: true
    showIndentGuide: true
    tabLength: 4
    tabType: "soft"
  "file-icons": {}
  fonts:
    fontFamily: "PT Mono"
  "fuzzy-finder":
    ignoredNames: [
      "storage/*"
      "public/assets/*"
      "public/build/*"
    ]
  "genesis-ui":
    distractionFree: {}
  "glowing-cursor": {}
  jshint: {}
  linter:
    errorPanelHeight: 200
    lintOnFly: false
    lintOnFlyInterval: 5000
  "linter-eslint": {}
  "linter-js-standard": {}
  "linter-phpcs":
    excludedSniffs: [
      "PSR2.ControlStructures.ControlStructureSpacing"
      "Generic.Files.LineLength"
      "Generic.ControlStructures.InlineControlStructure"
      "Generic.Functions.FunctionCallArgumentSpacing"
      "Squiz.Functions.MultilineFunctionDeclaration"
    ]
    executablePath: "/usr/local/bin/phpcs"
  minimap:
    plugins:
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      pigments: true
      pigmentsDecorationsZIndex: 0
  "one-dark-ui":
    fontSize: 11
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "php-cs-fixer":
    executablePath: "/Users/boris/.composer/vendor/fabpot/php-cs-fixer/php-cs-fixer"
    runPhpWithoutAnyIni: true
    showInfoNotifications: true
  "php-integrator-autocomplete-plus": {}
  "php-integrator-symbol-viewer": {}
  pigments: {}
  "project-manager":
    sortBy: "title"
  "remote-sync": {}
  "seti-ui":
    themeColor: "Green"
  "todo-show":
    ignoreThesePaths: [
      "**/node_modules/"
      "**/vendor/"
      "**/bower_components/"
      "resources/assets/vendor/kode/"
    ]
  "tree-view":
    hideIgnoredNames: true
    squashDirectoryNames: true
  welcome:
    showOnStartup: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
".html.php.text":
  editor:
    tabLength: 4
".js.regexp.replacement.source":
  editor:
    tabLength: 4
".js.regexp.source":
  editor:
    tabLength: 4
".js.source":
  editor:
    tabLength: 4
